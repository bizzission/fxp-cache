{
    "name": "sonatra/cache",
    "type": "library",
    "description": "Add extra features to Symfony PSR-6 Cache Component",
    "keywords": ["cache", "psr-6", "namespace", "prefix"],
    "homepage": "https://github.com/sonatra/sonatra-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": false,
    "require": {
        "php": ">=7.0.0",
        "symfony/cache": "^3.3.0"
    },
    "require-dev": {
        "symfony/filesystem": "^3.3.0",
        "doctrine/dbal": "^2.5.0",
        "predis/predis": "^1.1.0"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Component\\Cache\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
